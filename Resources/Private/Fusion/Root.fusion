include: Notification.fusion

prototype(Breadlesscode.Notification:Notification) < prototype(PackageFactory.AtomicFusion:Content) {
    renderer = Breadlesscode.Components:Notification {
        content = PackageFactory.AtomicFusion:Editable {
            property = 'content'
        }
        type = ${ q(node).property('type') }
        onlyRenderInBackend = ${ q(node).property('onlyRenderInBackend') }
    }
}
